<Application x:Class="lab3projectTeste.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:lab3projectTeste"        
             xmlns:viewModel="clr-namespace:lab3projectTeste.ViewModel"
             xmlns:view="clr-namespace:lab3projectTeste"    
             StartupUri="LoginPage.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <DataTemplate DataType="{x:Type viewModel:ListasViewModel}">
                <local:MinhasListasView/>
            </DataTemplate>
            

          
            <DataTemplate DataType="{x:Type viewModel:AboutProfileViewModel}">
                <local:AboutProfile/>
            </DataTemplate>
             

        </ResourceDictionary>
    </Application.Resources>
</Application>
